{
  "title" : "eternal-heathens个人文档",
  "plugins": [
			"summary",
			"splitter",
			"chapter-fold", 
			"expandable-chapters",
			"-lunr", "-search", "search-pro",
			"github",
			"prism", "-highlight",
			"tbfed-pagefooter",
			"accordion",
			"popup",
			"hide-element",
			"lightbox",
			"edit-link",
			"theme-default",
			"livereload",
			"back-to-top-button",
			"favicon",
			"anchor-navigation-ex"
		],
	"pluginsConfig": {
		"page-treeview": {
			"copyright": "Copyright &#169; aleen42",
			"minHeaderCount": "2",
			"minHeaderDeep": "3"
		},
		"github": {
				"url": "https://github.com/eternal-heathens?tab=repositories"
		},
		"prism": {
			"css": [
			  "prismjs/themes/prism-okaidia.css"
			  ]
		},
		"tbfed-pagefooter": {
				"copyright":"",
				"modify_label": "该文件最后修改时间：",
				"modify_format": "YYYY-MM-DD HH:mm:ss"
		},
		"hide-element": {
            "elements": [".gitbook-link"]
        },
		"edit-link": {
            "base": "//github.com/yulilong/book/edit/master",
            "label": "编辑此页面"
       },
	   "theme-default": {
            "showLevel": true
        },
		"favicon": {
            "shortcut": "asset/img/favicon.ico",
            "bookmark": "asset/img/favicon.ico",
            "appleTouch": "asset/img/favicon.ico",
            "appleTouchMore": {
                "120x120": "asset/img/favicon.ico",
                "180x180": "asset/img/favicon.ico"
            }
        },
		"anchor-navigation-ex": {
            "showLevel": false, 
            "showGoTop": false 
        }
		
  }
}

